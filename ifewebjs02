<script type="text/javascript">

var aqiData = [
  ["北京", 90],
  ["上海", 50],
  ["福州", 10],
  ["广州", 50],
  ["成都", 90],
  ["西安", 100]
];

(function () {

  /*
  在注释下方编写代码
  遍历读取aqiData中各个城市的数据
  将空气质量指数大于60的城市显示到aqi-list的列表中
  */
 var i,j,lists;
 var adnew=[];
 var listnum=['一','二','三'];
 var ulid=document.getElementById('aqi-list');
for(i=0;i<aqiData.length;i++){
  if(aqiData[i][1]>60){
   adnew.push({"city":aqiData[i][0],"num":aqiData[i][1]});
  }
}
function mysort(proname){
  return function(a,b){
    var val1=a[proname];
    var val2=b[proname];
    return val2-val1;
  };
}
adnew=adnew.mysort("num");
  for(j=0,j<3;j++){
   lists="<li>第"+listnum[j]+"名："+adnew[j]["city"]+", "+adnew[j]["num"];
   function addlist(){
     ulid.appendChild(lists);
   }
  }
})();

</script>
